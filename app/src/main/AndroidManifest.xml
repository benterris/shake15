<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.benjamin.android.shake15">

    <application
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/appicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.benjamin.android.shake15.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.benjamin.android.shake15.DisplayScoreActivity"
            android:parentActivityName="com.benjamin.android.shake15.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.benjamin.android.shake15.MainActivity" />
        </activity>
        <activity
            android:name="com.benjamin.android.shake15.GameActivity"
            android:parentActivityName="com.benjamin.android.shake15.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.benjamin.android.shake15.MainActivity" />
        </activity>
        <activity
            android:name="com.benjamin.android.shake15.HighScoreActivity"
            android:parentActivityName="com.benjamin.android.shake15.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.benjamin.android.shake15.MainActivity" />
        </activity>
    </application>

</manifest>